#include <iostream>

int* MaxSum(int* arr1, int size1, int* arr2, int size2) {
    int sum1 = 0;
    for (int i = 0; i < size1; ++i) {
        sum1 += arr1[i];
    }

    int sum2 = 0;
    for (int i = 0; i < size2; ++i) {
        sum2 += arr2[i];
    }

    return (sum1 > sum2) ? arr1 : arr2;
}

int main() {
    int arr1[] = {1, 2, 3};
    int arr2[] = {4, 5, 6};

    int* maxSumA = MaxSum(arr1, 3, arr2, 3);
    
    for (int i = 0; i < 3; ++i) {
        std::cout << maxSumA[i] << " ";
    }
    std::cout << std::endl;

    return 0;
}

